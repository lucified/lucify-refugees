{
  "publicRoot": "dist",
  "build": {
    "image": "starefossen/ruby-node",
    "commands": ["apt-get update && apt-get -y install gdal-bin && bundle install && npm install && ./prepare.sh", "NODE_ENV=production MINARD=true npm run build"],
    "cache": {
      "key": "%CI_PROJECT_PATH%",
      "paths": ["node_modules/", "vendor/bundle"]
    }
  }
}
